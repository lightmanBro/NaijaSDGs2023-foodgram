        </div>
        <div class="container-body">
            {{#each posts}}
            <div class="post-container">
                <div class="post-heading">
                    <div class="user">
                        <div class="user-image">
                            <img src="" alt="" srcset="">
                        </div>
                        <div class="user-name"><a href="./profile.html">Aminatu <span>yaba</span></a></div>
                    </div>
                    <div class="post-title">{{description}}</div>
                    <div class="more-options"><span class="material-symbols-outlined">
                            more_vert
                        </span></div>
                </div>
                <div class="post-body">
                    <img src="./img/Soaked Garri_ Nigeria’s Legendary ‘cereal’.jpeg" alt="">
                </div>
                <div class="post-actions">
                    <div class="orders-count"><span>₦</span>{{price}}</div>
                    <div class="orders-count"><span>Total</span> {{orderAvailable}}, <span> Available</span> {{orderLeft}}</div>
                    <button id="order-button" type="submit"><a href="./order.html">Order</a></button>
                </div>
                <div class="likes"> <span class="material-symbols-outlined">
                        favorite
                    </span> {{likes}} likes </div>
                <div class="time">2hr ago</div>
            </div>
                {{/each}}
        </div>
        <div class="add">
            Post
        </div>
        <div class="logo">
            <div class="food-logo">
                foodgram <span class="material-symbols-outlined">
                    restaurant_menu
                </span>
                
            </div>
            <div class="other-actions">
                <div class="messages"><a href=""> <span class="material-symbols-outlined"> notifications </span></a></div>
                <div class="orders"><a href="./catalogue.html"><span class="material-symbols-outlined"> orders </span></a></div>
                <div class="profile"><a href="./profile.html"><span class="material-symbols-outlined"> person </span></a></div>
            </div>
        </div>
        <div id="new-post" class="new-post hidden">
            <form >
                <div class="new-post-img">
                    <span class="material-symbols-outlined">
                        photo_camera
                        </span>
                    <input type="file" name="" id="">
                </div>
                <div class="input-box">
                    <input type="text" name="description" placeholder="Write description....">
                </div>
                <div class="post-buttons">
                    <button class="post-btn" type="submit">Post</button>
                    <button class="post-btn" type="reset">Cancel</button>
                </div>
            </form>
        </div>
    </div>
